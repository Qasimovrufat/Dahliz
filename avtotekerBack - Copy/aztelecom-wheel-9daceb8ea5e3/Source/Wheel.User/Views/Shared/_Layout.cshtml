    @*<div id="page_preloader"></div>*@


    <div id="fb-root"></div>
    <script>
(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8&appId=1571290463186394";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
    <div id="wrapper1">
        <div id="wrapper2">
            <!-- HEADER -->
            @Html.Partial("_Header")
            <!-- SHOWCASE CUSTOM BLOCKS -->
            <!-- BREADCRUMBS -->
            @if (Model.breadCrumb != null)
            {
                @:<div class="breadcrumb_wrap">
                    <div class="container">
                        <ul class="breadcrumb">
                            @foreach (var link in Model.breadCrumb)
                            {
                               
                                <li>
                                    @if (link.url != null)
                                    {
                                        @:<a href="@link.url" class="homepage-link" title="@link.name">@link.name</a>
                                        }
                                    else
                                    {
                                        @:<span class="page-title">@link.name</span>
                                         }
                                   
                                </li>
                            }


                        </ul>
                    </div>
                @:</div>
            }
            <!-- MAIN CONTENT -->
            @RenderBody()

            @Html.Partial("_Footer")
        </div><!-- / #wrapper2 -->
    </div><!-- / #wrapper1 -->
    <a id="back_top" title="Back to top" href="#"><i class="fa fa-angle-up"></i></a>
    <div class="cart_popup">Product Added to Cart</div>
    @Html.Partial("_Bottom")
    @RenderSection("bottom", required: false)
    @RenderSection("script", required: false)
</body>
</html>
